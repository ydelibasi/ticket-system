{% extends 'layout.html.twig' %}

{% block content %}
    <h2>Users</h2>
    <table>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>E-mail</td>
        </tr>
        {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.name }}</td>
                <td>{{ user.email }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}
